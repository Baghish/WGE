<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-type" content="text/html; charset=[% settings.charset %]" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>WGE</title>

    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/2.1.1/css/bootstrap.min.css">  
    <link rel="stylesheet" href="static/css/jquery-ui-1.9.2.custom.min.css" />

    <!-- Grab Google CDN's jQuery. fall back to local if necessary -->
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js" type="text/javascript"></script>
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.1/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="static/javascript/jquery-ui-1.9.2.custom.min.js"></script>

    <script type="text/javascript">
      function create_alert(text) {
          //create an error alert,
          //should make it so we can actually change he class
          $(".container").prepend(
              $("<div>", { class: "alert alert-danger alert-dismissable" })
                  .append( $("<button>", { class: "close", type: "button", 'aria-hidden': "true", html: "&times;", 'data-dismiss': "alert" }) )
                  .append( $("<span>", { html: text }) )
          );
      }
    </script>

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
</head>
<body>

    <div class="navbar">
        <div class="navbar-inner">
          <a class="brand" href="#">HTGT WGE 
              [%- IF settings.environment == 'development' -%]
                [ DEVEL ]
              [%- END -%]
          </a>
          <ul class="nav">
            <li class="active"><a href="[% c.uri_for('/') %]">Home</a></li>
            <li><a href="[% c.uri_for('/gibson_design_gene_pick') %]">Gibson Designs</a></li>
            <li><a href="[% c.uri_for('/about') %]">About</a></li>
            <li><a href="[% c.uri_for('/contact') %]">Contact</a></li>
          </ul>
        </div>
    </div>

    <div class="container">
        <div class="alert alert-danger alert-dismissable" id="alert_error" style="display:none">
          <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
          <span></span>
        </div>

        <!-- catalyst error message disply taken from LIMS2 -->
        [% SET error = error_msg || c.flash.error_msg; IF error.length > 0 %]
        <div class="alert alert-error">
          <button class="close" data-dismiss="alert" href="#">&times;</button>
          <p>[% error %]</p>
        </div>
        [% END %]
        [% SET info = info_msg || c.flash.info_msg; IF info.length > 0 %]
        <div class="alert alert-info">
          <button class="close" data-dismiss="alert" href="#">&times;</button>
          <p>[% info %]</p>
        </div>
        [% END %]
        [% SET success = success_msg || c.flash.success_msg; IF success.length > 0 %]
        <div class="alert alert-success">
          <button class="close" data-dismiss="alert" href="#">&times;</button>
          <p>[% success | none %]</p>
        </div>
        [% END %]


        [% content %]
    </div>
</body>
</html>
