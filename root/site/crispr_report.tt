[% META title="Crispr Report" %]

<div class="page-header">
  <h1>Crispr
  <small>[% crispr.id %]</small>
  </h1>
</div>

<table class="table table-bordered">
  [%- FOR r IN display_items %]
  [%- SET title=r.0; SET accessor = r.1 -%]
  <tr>
    <th>[% title %]</th> <td>[% crispr.$accessor %]</td>
  </tr>
  [%- END %]
</table>