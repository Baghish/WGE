use strict;
use warnings;
use Log::Log4perl qw(:easy);

Log::Log4perl->easy_init($TRACE);

use WGE::Model::DB;

my $model = WGE::Model::DB->new();

my @chr = $model->resultset('Chromosome')->all;
print join "\n", map {$_->name} @chr;

check($model,{chr_name =>'grr'},{ chr_name => { validate => 'existing_chromosome' }});

sub check{
    my ($model, @args) = @_;
    $model->check_params(@args);
}
